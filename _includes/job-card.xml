  <entry>
    <title>{{ title | xml_escape }}</title>
    <link rel="alternate" type="text/html" href="{{ url }}" />
    <published>{{ postdate | date_to_xmlschema }}</published>
    <updated>{{ postdate | date_to_xmlschema }}</updated>
    <id>{{ url }}</id>
    <content type="html">
      {{ description }}
    </content>
  </entry>
